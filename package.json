{
  "name": "cmc-ccd-e2e-tests",
  "version": "1.0.0",
  "description": "End to End CCD tests for CMC",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --fix",
    "test:debug": "npx playwright test --project full-functional --grep @debug",
    "test:full-functional": "npx playwright test --project full-functional",
    "test:functional": "npx playwright test --project full-functional",
    "test:fullfunctional": "yarn playwright install && (yarn test:functional ; yarn test:functional:report)",
    "test:functional:report": "allure generate ./allure-results --clean && allure open ./allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmcts/cmc-ccd-e2e-tests.git"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@axe-core/playwright": "^4.9.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "dependency-check": "^4.1.0",
    "dotenv": "^16.4.5",
    "html_codesniffer": "^2.5.1",
    "node-cache": "^5.1.2",
    "playwright-core": "^1.43.0",
    "require-directory": "^2.1.1",
    "typescript": "^5.4.5",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@hmcts/div-idam-test-harness": "^1.6.0",
    "@playwright/test": "^1.43.0",
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "allure-commandline": "^2.29.0",
    "allure-playwright": "2.4.0",
    "codeceptjs": "^3.3.7",
    "eslint": "8.57.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "infinite-sequence-generator": "^1.1.0",
    "launchdarkly-node-server-sdk": "^5.13.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^22.7.1",
    "randomstring": "^1.1.5",
    "sinon": "^5.0.7",
    "totp-generator": "^1.0.0",
    "urlencode": "^1.1.0",
    "uuid": "^8.3.2",
    "webdriverio": "^6.11.3"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmcts/cmc-ccd-e2e-tests/issues"
  },
  "homepage": "https://github.com/hmcts/cmc-ccd-e2e-tests#readme",
  "packageManager": "yarn@3.2.4"
}
